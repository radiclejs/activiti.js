{
  "name": "koa-seed",
  "version": "0.2.0",
  "description": "a basic framework seed project base on koa && eggjs",
  "main": "app.js",
  "author": "Muggle Team",
  "license": "ISC",
  "private": true,
  "bin": {
    "mocha": "node_modules/mocha/bin/mocha"
  },
  "scripts": {
    "start": "pm2 start ./ecosystem.config.js --env production",
    "stop": "pm2 stop lottery_app lottery_agent",
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w",
    "dev": "cross-env EGG_SERVER_ENV=local nodemon ./app_setup.js",
    "dev:agent": "cross-env EGG_SERVER_ENV=local nodemon ./agent_setup.js",
    "debug": "cross-env EGG_SERVER_ENV=local nodemon --inspect ./app_setup.js",
    "debug:agent": "cross-env EGG_SERVER_ENV=local nodemon --inspect ./agent_setup.js",
    "debug:first": "cross-env EGG_SERVER_ENV=local nodemon --inspect --inspect-brk ./app_setup.js",
    "debug:agent:first": "cross-env EGG_SERVER_ENV=local nodemon --inspect --inspect-brk ./agent_setup.js",
    "changelog": "standard-version",
    "release": "yarn changelog",
    "lint": "eslint app framework config lib test *.js",
    "test": "yarn lint -- --fix && yarn test-local",
    "test:local": "cross-env EGG_SERVER_ENV=unittest mocha --require intelli-espower-loader ./test/**/*.test.js",
    "test:framework": "cross-env EGG_SERVER_ENV=unittest mocha --require intelli-espower-loader ./framework/**/*.test.js",
    "nyc:report": "nyc report --reporter=text-summary",
    "cov": "nyc yarn test:local && yarn nyc:report",
    "cov:framework": "nyc yarn test:framework  && yarn nyc:report",
    "ci": "yarn lint && yarn cov",
    "migrate:seed:local": "cross-env EGG_SERVER_ENV=local node ./bin/db-seed.js",
    "migrate:seed:test": "cross-env EGG_SERVER_ENV=unittest node ./bin/db-seed.js",
    "migrate:seed:prod": "cross-env EGG_SERVER_ENV=prod node ./bin/db-seed.js",
    "migrate:new": "egg-sequelize migration:create",
    "migrate:up": "egg-sequelize db:migrate",
    "migrate:down": "egg-sequelize db:migrate:undo"
  },
  "devDependencies": {
    "autocannon": "^1.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "csvjson": "^5.0.0",
    "egg": "^2.3.0",
    "eslint": "^4.14.0",
    "faker": "^4.1.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^4.1.0",
    "nodemon": "^1.14.3",
    "nyc": "^11.4.1",
    "pm2": "^2.9.1",
    "power-assert": "^1.4.4",
    "standard-version": "^4.2.0"
  },
  "dependencies": {
    "camunda-bpmn-moddle": "^2.0.0",
    "cross-env": "^5.1.3",
    "debug": "^3.1.0",
    "delegates": "^1.0.0",
    "egg-cookies": "^2.2.1",
    "egg-core": "^4.3.0",
    "egg-cors": "^2.0.0",
    "egg-development": "^2.0.0",
    "egg-i18n": "^2.0.0",
    "egg-jsonp": "^2.0.0",
    "egg-logger": "^1.6.0",
    "egg-logrotator": "^3.0.0",
    "egg-mock": "^3.14.0",
    "egg-multipart": "^2.0.0",
    "egg-oauth2-server": "^2.1.2",
    "egg-onerror": "^2.0.0",
    "egg-passport": "^2.0.1",
    "egg-redis": "^1.0.2",
    "egg-schedule": "^3.1.0",
    "egg-security": "^2.0.0",
    "egg-sequelize": "git://github.com/xingyuzhe/egg-sequelize.git#eeb73897b95918b3bdf7376565a9ed2f83b562fa",
    "egg-session": "^3.0.0",
    "egg-static": "^2.0.0",
    "egg-validate": "^1.0.0",
    "egg-view": "^2.0.0",
    "egg-view-nunjucks": "^2.1.4",
    "egg-watcher": "^3.0.0",
    "jsonfile": "^4.0.0",
    "koa-bodyparser": "^4.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "mysql2": "^1.5.1",
    "passport-local": "^1.0.0",
    "pmx": "^1.5.5",
    "request": "^2.83.0",
    "socket.io": "^2.0.4",
    "socket.io-redis": "^5.2.0",
    "uuid": "^3.1.0"
  }
}
